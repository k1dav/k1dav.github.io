{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "npm: start",
      "type": "shell",
      "command": "npm",
      "args": ["start"],
      "options": {
        "cwd": "${workspaceFolder}/app/"
      },
      "group": {
        "kind": "develope",
        "isDefault": true
      }
    },
    {
      "label": "npm: build",
      "type": "shell",
      "command": "npm",
      "args": ["run", "build"],
      "options": {
        "cwd": "${workspaceFolder}/app/"
      },
      "group": {
        "kind": "build",
        "isDefault": false
      }
    }
  ]
}
